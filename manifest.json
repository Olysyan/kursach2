{
  "manifest_version": 2,
  "name": "NS Fingerprint shelter",
  "description": "NS Fingerprint shelter",
  "version": "1.0.0",
  "icons": {
    "128": "icon-128.png"
  },
  "permissions": [
    "<all_urls>",
    "webRequest",
    "webRequestBlocking",
    "storage"
  ],
  "background": {
    "scripts": [
      "data/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "data/content.js"
      ],
      "run_at": "document_start",
      "all_frames": true,
      "match_about_blank": true
    }
  ],
  "browser_action": {
    "default_popup": "popup/popup.html"
  }
}